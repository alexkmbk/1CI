		<dataSource>DataSource1</dataSource>
		<query>SELECT
	Users.Ref,
	FALSE AS IsExternalUser,
	CASE
		WHEN Users.InfobaseUserID IN (&amp;NonExistentInfobaseUserIDs)
			THEN TRUE
		ELSE FALSE
	END AS NoMatchingInfobaseUser,
	Users.DeletionMark,
	Users.Description,
	Users.InfobaseUserID,
	Users.NotValid,
	Users.Comment,
	Users.Predefined
	FROM
	Catalog.Users AS Users
	WHERE
	Users.Internal = FALSE
	AND &amp;Users

UNION ALL

SELECT
	ExternalUsers.Ref,
	TRUE,
	CASE
		WHEN ExternalUsers.InfobaseUserID IN (&amp;NonExistentInfobaseUserIDs)
			THEN TRUE
		ELSE FALSE
	END,
	ExternalUsers.DeletionMark,
	ExternalUsers.Description,
	ExternalUsers.InfobaseUserID,
	ExternalUsers.NotValid,
	ExternalUsers.Comment,
	ExternalUsers.Predefined
	FROM
	Catalog.ExternalUsers AS ExternalUsers
	WHERE
		&amp;ExternalUsers</query>
