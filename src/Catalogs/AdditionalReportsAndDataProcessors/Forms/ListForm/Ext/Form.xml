	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.ObjectName</Field>
				<Field>List.Ref</Field>
				<Field>List.IsFolder</Field>
				<Field>List.IsReport</Field>
				<Field>List.FileName</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Table.Ref,
	Table.DeletionMark,
	Table.Description,
	Table.Version,
	Table.Responsible,
	Table.Publication,
	Table.Kind,
	Table.ObjectName,
	Table.IsFolder,
	CASE
		WHEN Table.Kind IN (&amp;AdditionalReportKinds)
			THEN TRUE
		ELSE FALSE
	END AS IsReport,
	Table.FileName
	FROM
	Catalog.AdditionalReportsAndDataProcessors AS Table
	WHERE
	CASE
			WHEN Table.IsFolder
				THEN TRUE
			WHEN &amp;PublicationFilter = UNDEFINED
				THEN TRUE
			WHEN &amp;PublicationFilter = 1
				THEN Table.Publication IN (&amp;AllPublicationsExceptUnused)
			ELSE Table.Publication = &amp;PublicationFilter
		END
	AND CASE
			WHEN Table.IsFolder
				THEN TRUE
			WHEN &amp;KindFilter = UNDEFINED
				THEN TRUE
			WHEN &amp;KindFilter = 1
				THEN Table.Kind IN (&amp;AdditionalReportKinds)
			WHEN &amp;KindFilter = 2
				THEN Not Table.Kind IN (&amp;AdditionalReportKinds)
			ELSE Table.Kind = &amp;KindFilter
			END</QueryText>
				<MainTable>Catalog.AdditionalReportsAndDataProcessors</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
		</Attribute>
