			<UseAlways>
				<Field>List.Ref</Field>
				<Field>List.Description</Field>
				<Field>List.Parent</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	IDs.Ref,
	IDs.DataVersion,
	IDs.DeletionMark,
	CASE
		WHEN SUBSTRING(IDs.Name, 0, 7) = "Delete"
			THEN TRUE
		ELSE FALSE
	END AS FutureDeletionMark,
	IDs.Predefined,
	IDs.Parent,
	IDs.Description,
	IDs.CollectionOrder,
	IDs.Name,
	IDs.Synonym,
	IDs.FullName,
	IDs.FullSynonym,
	IDs.WithoutData,
	IDs.EmptyRefValue,
	IDs.MetadataObjectKey
	FROM
			Catalog.MetadataObjectIDs AS IDs</QueryText>
				<MainTable>Catalog.MetadataObjectIDs</MainTable>
