							<dcsset:left xsi:type="dcscor:Field">DataParameters.VersionFrom</dcsset:left>
							<dcsset:comparisonType>NotEqual</dcsset:comparisonType>
							<dcsset:right xsi:type="xs:string">0.0.0.0</dcsset:right>
						</dcsset:item>
						<dcsset:item xsi:type="dcsset:FilterItemComparison">
							<dcsset:left xsi:type="dcscor:Field">VersionWeight</dcsset:left>
							<dcsset:comparisonType>Greater</dcsset:comparisonType>
